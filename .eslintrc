{
  "env": {
    "node": 1,
    "browser": 0
  },
  "globals": {

  },
  "rules": {
    "quotes": [2, "single", "avoid-escape"],
    "eqeqeq": 0,
    "brace-style": [2, "stroustrup"],
    "eol-last": 0,
    "no-nested-ternary": 1,
    "padded-blocks": [1, "never"],
    "space-after-function-name": [1, "always"],
    "space-before-blocks": [1, "always"]
  },
  "plugins": [

  ]
}
